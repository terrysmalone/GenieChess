<!-- All scores are added to the respective side (i.e. black aims for a negative.-->
<!-- Penalties should be added as negative values -->
<ScoreSet Name="ScoreValues">
  <!-- Piece scores - scores for simply having the pieces-->
  <PawnPieceValue>1</PawnPieceValue>
  <KnightPieceValue>3</KnightPieceValue>
  <BishopPieceValue>3.2</BishopPieceValue>
  <RookPieceValue>5</RookPieceValue>
  <QueenPieceValue>9</QueenPieceValue>

  <!-- Scores for having each piece near to the middle of the board -->
  <InnerCentralPawnScore>0.2</InnerCentralPawnScore> <!-- Score for having the piece in the middle 4 squares of the board -->
  <OuterCentralPawnScore>0.05</OuterCentralPawnScore> <!-- Score for having the piece in the outer ring (12 squars) of the middle 16 squares of the board -->

  <InnerCentralBishopScore>0.05</InnerCentralBishopScore>
  <OuterCentralBishopScore>0.1</OuterCentralBishopScore> 

  <InnerCentralKnightScore>0.05</InnerCentralKnightScore> 
  <OuterCentralKnightScore>0.1</OuterCentralKnightScore> 

  <InnerCentralRookScore>0.01</InnerCentralRookScore> 
  <OuterCentralRookScore>0.01</OuterCentralRookScore> 

  <InnerCentralQueenScore>0.05</InnerCentralQueenScore>
  <OuterCentralQueenScore>0.05</OuterCentralQueenScore>
  
  <!-- Castling scores -->

  <CastlingKingSideScore>0.25</CastlingKingSideScore> <!-- Score for castling kingside -->
  <CastlingQueenSideScore>0.15</CastlingQueenSideScore> <!-- Score for castling queenside -->

  <CanCastleQueensideScore>0.05</CanCastleQueensideScore> <!-- Score for still being able to castle kingside -->
  <CanCastleKingsideScore>0.06</CanCastleKingsideScore>  <!-- Score for still being able to castle queenside -->

